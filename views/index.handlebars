<div class="cards">
  <div class="card">
    <h2>To Be Devoured</h2>
    <ul>
      {{#each burgers}}
        {{#unless devoured}}
        {{> burger-block }}
        {{/unless}}
      {{/each}}
    </ul>
  </div>

  <div class="card">
    <h2>Devoured</h2>
    <ul>
      {{#each burgers}}
        {{#if devoured}}
          {{> burger-block }}
        {{/if}}
      {{/each}}
    </ul>
  </div>
</div>

